{
	"name": "zerotwo-discordbot",
	"description": "zerotwo-discordbot",
	"main": "./dist/index.js",
	"version": "0.1.1",
	"scripts": {
		"build": "rimraf dist && tsc",
		"start": "cross-env NODE_ENV=production node --trace-warnings .",
		"start:dev": "yarn lint && cross-env NODE_ENV=development node --trace-warnings .",
		"dev": "tsc-watch --onSuccess \"yarn start:dev\"",
		"lint": "eslint . --ext .ts"
	},
	"dependencies": {
		"@discordjs/opus": "^0.3.3",
		"@types/node-schedule": "^1.3.1",
		"cors": "^2.8.5",
		"cross-env": "^7.0.2",
		"discord-player": "^3.0.2",
		"discord.js": "^12.5.1",
		"discordjs-prompter": "^2.0.4",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"ffmpeg-static": "^4.2.7",
		"genius-api": "^0.4.1",
		"moment": "^2.29.1",
		"mongoose": "5.10.15",
		"morgan": "^1.10.0",
		"nhentai-api": "^3.0.2",
		"ytdl-core-discord": "^1.2.4"
	},
	"devDependencies": {
		"@types/cors": "^2.8.8",
		"@types/express": "^4.17.9",
		"@types/mongodb": "^3.5.33",
		"@types/mongoose": "^5.10.0",
		"@types/morgan": "^1.9.2",
		"@types/node": "^14.14.7",
		"@types/node-fetch": "^2.5.7",
		"@typescript-eslint/eslint-plugin": "^4.11.0",
		"@typescript-eslint/parser": "^4.11.0",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^7.1.0",
		"rimraf": "^3.0.2",
		"tsc-watch": "^4.2.9",
		"typescript": "^4.0.5"
	},
	"engines": {
		"node": "14.x.x",
		"yarn": "1.22.10"
	}
}
