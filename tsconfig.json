{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"esnext"
		],
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "esnext",
		"rootDir": "./src",
		"outDir": "./dist",
		"typeRoots": [
			"@types",
			"node_modules/@types"
		]
	}
}